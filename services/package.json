{
  "name": "jubo-doctor",
  "version": "1.0.1",
  "description": "jubo-doctor-server",
  "author": "11",
  "contributors": [
    "11"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/libterty/jubo-doctor.git"
  },
  "engines": {
    "node": ">=10"
  },
  "private": false,
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/libterty/jubo-doctor/LICENCE"
    }
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@jest-mock/express": "^1.4.3",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/cqrs": "^7.0.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/microservices": "^7.6.5",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-fastify": "^7.5.1",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/terminus": "^7.1.2",
    "@nestjs/typeorm": "^7.1.4",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "codecov": "^3.8.1",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "fastify-cors": "^4.1.0",
    "fastify-swagger": "^3.5.0",
    "helmet": "^4.6.0",
    "lodash": "^4.17.20",
    "mysql": "^2.18.1",
    "nanoid": "^3.1.20",
    "proxy-addr": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "request-promise": "^4.2.6",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.34",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.6.15",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "4.17.3",
    "@types/express-serve-static-core": "4.17.18",
    "@types/jest": "26.0.10",
    "@types/lodash": "^4.14.165",
    "@types/node": "^13.9.1",
    "@types/proxy-addr": "^2.0.0",
    "@types/request-promise": "^4.1.46",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.5",
    "jest": "26.4.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
